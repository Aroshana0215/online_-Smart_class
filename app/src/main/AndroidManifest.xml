<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gurugedara">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Gurugedara">
        <activity
            android:name=".ActivitySupport"
            android:label="Support Service" />


        <activity android:name=".UpdatePayment"
            android:label="UPDATE PAYMENT"
            android:parentActivityName=".Payment"/>
        <activity
            android:name=".AddPayment"
            android:label="ADD PAYMENT"
            android:parentActivityName=".Payment" />
        <!-- <activity android:name=".Payment" -->
        <!-- android:label="DO PAYMENT" /> -->
        <activity
            android:name=".UpdateQuestion"
            android:label="UPDATE QUESTION"
            android:parentActivityName=".QestioBox" />
        <activity
            android:name=".AddQuestion"
            android:label="ADD QUESTION"
            android:parentActivityName=".QestioBox" />
        <!-- <activity android:name=".QestioBox"  android:label="QUESTION BOX"></activity> -->


        <activity
            android:name=".UpdateNote"
            android:parentActivityName=".ActivityNote" />
        <activity
            android:name=".AddNote"
            android:label="Add Note"
            android:parentActivityName=".ActivityNote" />
        <activity
            android:name=".ActivityNote"
            android:label="Notes" />

        <activity android:name=".MainActivity" >
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>

    </application>

</manifest>